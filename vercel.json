{
  "version": 2,
  "routes": [
    {
      "src": "/pedidos",
      "methods": ["GET"],
      "dest": "/api/server.js"
    },
    {
      "src": "/pedido",
      "methods": ["POST"],
      "dest": "/api/server.js"
    },
    {
      "src": "/pedido/:id",
      "methods": ["PUT"],
      "dest": "/api/server.js"
    }
  ],
  "functions": {
    "server": {
      "runtime": "node@14.17.0"
    }
  }
}
